# Path to the directory containing images to upload
IMAGES_DIR=./images/photos/

# S3 endpoint URL (e.g., http://localhost:4566 for LocalStack, or leave blank for AWS)
S3_ENDPOINT_URL=http://localhost:4566

# AWS credentials (use dummy values for LocalStack, real values for AWS)
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_DEFAULT_REGION=us-east-1

# Name of the S3 bucket to upload images to
BUCKET_NAME=sac-image-gallery